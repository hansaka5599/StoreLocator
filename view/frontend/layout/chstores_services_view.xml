<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="CameraHouse\StoreLocator\Block\Services\View" name="store.services.view"
                   template="CameraHouse_StoreLocator::services/view.phtml"/>
        </referenceContainer>
        <referenceContainer name="columns.top">
            <block class="CameraHouse\StoreLocator\Block\Services\Back" name="store.page.title"
                   template="CameraHouse_StoreLocator::services/title.phtml"/>
        </referenceContainer>
        <referenceBlock name="page.main.title" remove="true"/>
    </body>

</page>
