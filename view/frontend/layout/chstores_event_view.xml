<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="CameraHouse\StoreLocator\Block\Events\View" name="store.event.view"
                   template="CameraHouse_StoreLocator::events/view.phtml"/>
        </referenceContainer>
        <referenceContainer name="columns.top">
            <block class="CameraHouse\StoreLocator\Block\Events\Back" name="store.page.title"
                   template="CameraHouse_StoreLocator::events/title.phtml"/>
        </referenceContainer>
        <referenceBlock name="page.main.title" remove="true"/>
    </body>
</page>
