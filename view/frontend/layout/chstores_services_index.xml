<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="CameraHouse\StoreLocator\Block\Services\Index" name="store.services.list"
                   template="CameraHouse_StoreLocator::services/list.phtml"/>
        </referenceContainer>
        <referenceContainer name="columns.top">
            <block class="CameraHouse\StoreLocator\Block\Services\BackServices" name="back.services.view"
                   template="CameraHouse_StoreLocator::services/back.phtml" after="page.main.title"/>
        </referenceContainer>
    </body>

</page>
