<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="store_locator_edit_tabs">
            <block class="CameraHouse\StoreLocator\Block\Adminhtml\Store\Edit\Tab\Home" name="store_locator_edit_home"/>
            <action method="addTab">
                <argument name="name" xsi:type="string">home_section</argument>
                <argument name="block" xsi:type="string">store_locator_edit_home</argument>
            </action>
        </referenceBlock>

        <referenceBlock name="store_locator_edit_banner" remove="true"/>
    </body>
</page>
